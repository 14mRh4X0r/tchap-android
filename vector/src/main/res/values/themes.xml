<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- LIGHT THEME COLORS -->
    <style name="Theme.Vector.Light" parent="@style/Theme.AppCompat.Light">
        <!-- default background color -->
        <item name="default_background_color">@android:color/white</item>

        <!-- waiting view background -->
        <item name="waiting_background_color">#AAAAAAAA</item>

        <!-- primary dark color (status bar) -->
        <item name="primary_dark_color">#5EA584</item>

        <!-- application bar background color -->
        <item name="primary_color">#76CFA6</item>

        <!-- application bar text color -->
        <item name="primary_text_color">@android:color/white</item>

        <!-- application bar icons color -->
        <item name="primary_control_color">@android:color/white</item>

        <!-- application bar text hint color -->
        <item name="primary_hint_text_color">#BBE7D3</item>

        <!-- default text colors -->
        <item name="default_text_color">@android:color/black</item>

        <!-- room message colors -->
        <item name="unsent_message_text_color">#FFFF4444</item>
        <item name="message_text_color">@android:color/black</item>
        <item name="encrypting_message_text_color">@color/vector_green_color</item>
        <item name="sending_message_text_color">#AAA</item>
        <item name="highlighted_message_text_color">@color/vector_fuchsia_color</item>
        <item name="highlighted_searched_message_text_color">@color/vector_green_color</item>





        <item name="vector_green_color">#FF76cfa6</item>
        <item name="vector_light_gray_color">#FFF2F2F2</item>
        <item name="vector_silver_color">#FFC7C7C7</item>

        <item name="vector_text_black_color">#FF3C3C3C</item>



        <!-- 0_54_black gives invalid rendering so use 9D gray -->
        <item name="vector_0_54_black_color">#8E000000</item>
        <item name="vector_0_87_black_color">#DC000000</item>
        <item name="vector_line_separator_gray_color">#339D9D9D</item>
        <item name="vector_recents_header_background_color">#77F2F2F2</item>
        <item name="vector_members_count_color">#8E000000</item>

        <!-- tabbar color-->
        <item name="vector_tabbar_background_color">#FFF2F2F2</item>
        <item name="vector_search_hint_color">#333C3C3C</item>

        <item name="vector_image_tint">?attr/primary_color</item>

        <!-- colors to check their uses -->

        <!-- NavigationView color-->
        <item name="vector_4d_gray">#FF4D4D4D</item>

        <!-- FloatingActionButton -->
        <item name="vector_fuchsia_color">#ff0064</item>

        <item name="vector_text_gray_color">#FF9D9D9D</item>

        <!-- tabbar underline -->
        <item name="vector_dc_gray">#FFDCDCDC</item>

        <!-- room members list -->
        <item name="vector_21_black">#FF212121</item>
        <item name="vector_80_gray">#FF808080</item>
        <item name="vector_05_gray">#FFF2F2F2</item>

        <item name="vector_d8_gray">#FFD8D8D8</item>

        <item name="vector_74_gray">#FF747474</item>
        <item name="vector_33_black">#33000000</item>

        <item name="vector_white_alpha_50">#80ffffff</item>

        <item name="list_header_background">#f6f6f6</item>
        <item name="list_header_spinner_text">#7f3c3c3c</item>
        <item name="list_header_spinner">#2e3c3c3c</item>

        <item name="vector_tabbar_selected_background_color">?attr/vector_green_color</item>
        <item name="vector_tabbar_unselected_background_color">?attr/vector_green_color</item>

        <item name="vector_deemed_background_view_gray_color">#339D9D9D</item>
    </style>

    <!-- DARK THEME COLORS -->
    <style name="Theme.Vector.Dark" parent="@style/Theme.AppCompat">
        <!-- default background color -->
        <item name="default_background_color">#2d2d2d</item>

        <!-- waiting view background -->
        <item name="waiting_background_color">#55555555</item>

        <!-- status bar color -->
        <item name="primary_dark_color">#191919</item>

        <!-- application bar color -->
        <item name="primary_color">#383838</item>

        <!-- application bar text color -->
        <item name="primary_text_color">@android:color/white</item>

        <!-- application bar icons color -->
        <item name="primary_control_color">#74CAA2</item>

        <!-- application bar text hint color -->
        <item name="primary_hint_text_color">#BBE7D3</item>

        <!-- default text colors -->
        <item name="default_text_color">@android:color/white</item>

        <!-- room message colors -->
        <item name="unsent_message_text_color">#FFFF4444</item>
        <item name="message_text_color">@android:color/white</item>
        <item name="encrypting_message_text_color">@color/vector_green_color</item>
        <item name="sending_message_text_color">#AAA</item>
        <item name="highlighted_message_text_color">@color/vector_fuchsia_color</item>
        <item name="highlighted_searched_message_text_color">@color/vector_green_color</item>


        <item name="vector_green_color">#46af7f</item>
        <!-- Lines just above the input field in rooms -->
        <item name="vector_light_gray_color">#565656</item>
        <!-- Icons on the bottom of the overview activity -->
        <item name="vector_silver_color">#FFC7C7C7</item>

        <!-- Names of people, rooms and categories in the rooms overview -->
        <!-- Also of headers (and filenames) in Room Details and People Details -->
        <item name="vector_text_black_color">#d9d9d9</item>


        <!-- 0_54_black gives invalid rendering so use 9D gray -->
        <!-- Display name and text in input field in rooms and text in room overview -->
        <item name="vector_0_54_black_color">#A4A4A4</item>
        <item name="vector_0_87_black_color">#DC000000</item>
        <!-- Line separator between rooms in overview -->
        <item name="vector_line_separator_gray_color">#c5c1c1</item>
        <!-- Room details, Search and Person details headers -->
        <item name="vector_recents_header_background_color">#5A5A5A</item>
        <item name="vector_members_count_color">#FFF</item>

        <!-- tabbar color-->
        <item name="vector_tabbar_background_color">?attr/default_background_color</item>
        <item name="vector_search_hint_color">#CCC3C3C3</item>

        <item name="vector_image_tint">?attr/primary_color</item>
        <!-- colors to check their uses -->

        <!-- NavigationView color-->
        <!-- Items from the NavigationView -->
        <item name="vector_4d_gray">#FF4D4D4D</item>

        <!-- FloatingActionButton -->
        <item name="vector_fuchsia_color">#ff0064</item>

        <!-- The display name of senders in the room overview -->
        <item name="vector_text_gray_color">#a0a29f</item>

        <!-- tabbar underline -->
        <!-- Separator between People and Files items in Room Details -->
        <item name="vector_dc_gray">#565656</item>

        <!-- room members list -->
        <!-- Display names of the room members -->
        <item name="vector_21_black">#DEDEDE</item>
        <!-- Last time a room member was online -->
        <item name="vector_80_gray">#A5A5A5A5</item>
        <item name="vector_05_gray">#CCF2F2F2</item>


        <item name="vector_d8_gray">#FFD8D8D8</item>

        <item name="vector_74_gray">#FF747474</item>
        <item name="vector_33_black">#33000000</item>

        <!-- Hint in the search bar -->
        <item name="vector_white_alpha_50">#80ffffff</item>

        <!-- Background of the headers in the room overview -->
        <item name="list_header_background">#5A5A5A</item>
        <!-- Name of the Room Directory that is currently selected -->
        <item name="list_header_spinner_text">#FFC7C7C7</item>
        <!-- Carrot and underlining of the name of the Room Directory that is currently selected above -->
        <item name="list_header_spinner">#A5A5A5A5</item>



        <item name="vector_tabbar_selected_background_color">#2d2d2d</item>
        <item name="vector_tabbar_unselected_background_color">#2d2d2d</item>



        <item name="vector_deemed_background_view_gray_color">#339D9D9D</item>
    </style>

</resources>
